<?xml version="1.0"?>

<robot name="phantomx_pincher" xmlns:xacro="http://ros.org/wiki/xacro">

<!-- -|-|-|-|-|-|-|-|- INCLUDE -|-|-|-|-|-|-|-|- -->

<xacro:include filename="$(find phantomx_pincher)/urdf/phantomx_parameters.xacro" />

<!-- -|-|-|-|-|-|-|-|- ROBOT DESCRIPTION -|-|-|-|-|-|-|-|- -->

<!-- Gazebo World link -->
<link name="world"/>  
<joint name="fixed" type="fixed">
    <parent link="world"/>
    <child link="link_base"/>
</joint>

<!-- Base Link -->
<link_phantomx 
    name="link_base"
    origin_rpy="0 0 0" 
    origin_xyz="0 0 0"
    inertial_rpy="0 0 0" 
    inertial_xyz="-0.026970000 0.000098000 0.043868000"
    meshfile="package://phantomx_pincher/meshes/link_base.dae"
    mass="${link_base_mass}"
    ixx="${link_base_ixx}" ixy="${link_base_ixy}" ixz="${link_base_ixz}"
    iyy="${link_base_iyy}" iyz="${link_base_iyz}" izz="${link_base_izz}"	
/>  

<!-- Joint-Servomotor 1 -->
<joint_phantomx
    name="joint_1" 
    type="revolute"
    parent="link_base" 
    child="link_1"
    origin_rpy="0 0 0" 
    origin_xyz="0 0 ${l0}"
    axis_xyz="0 0 1"
    limit_e="${le}" limit_l="${joint_lower}" limit_u="${joint_upper}" limit_v="${lv}" 
/>

<!-- Link 1 -->
<link_phantomx
    name="link_1"
    origin_rpy="0 0 0" 
    origin_xyz="0 0 0"
    inertial_rpy="0 0 0" 
    inertial_xyz="0.000032000 0.001031000 0.027694000"
    meshfile="package://phantomx_pincher/meshes/link_1.dae"
    mass="${link_1_mass}"
    ixx="${link_1_ixx}" ixy="${link_1_ixy}" ixz="${link_1_ixz}"
    iyy="${link_1_iyy}" iyz="${link_1_iyz}" izz="${link_1_izz}"	
/>  


<!-- Joint-Servomotor 2 -->
<!--origin_rpy - fixed angles-->
<joint_phantomx
    name="joint_2" 
    type="revolute"
    parent="link_1" 
    child="link_2"
    origin_rpy="-${M_PI/2} -${M_PI/2} 0" 
    origin_xyz="0 0 ${l1}"
    axis_xyz="0 0 1"
    limit_e="${le}" limit_l="${joint_lower}" limit_u="${joint_upper}" limit_v="${lv}" 
/>

<!-- Link 2 -->
<link_phantomx
    name="link_2"
    origin_rpy="${M_PI/2} 0 ${M_PI/2}" 
    origin_xyz="0 0 0"
    inertial_rpy="${M_PI/2} 0 ${M_PI/2}" 
    inertial_xyz="0.080988000 0.000023000 0.000748000"
    meshfile="package://phantomx_pincher/meshes/link_2.dae"
    mass="${link_2_mass}"
    ixx="${link_2_ixx}" ixy="${link_2_ixy}" ixz="${link_2_ixz}"
    iyy="${link_2_iyy}" iyz="${link_2_iyz}" izz="${link_2_izz}"	
/>  

<!-- Joint-Servomotor 3 -->
<joint_phantomx
    name="joint_3" 
    type="revolute"
    parent="link_2" 
    child="link_3"
    origin_rpy="0 0 0" 
    origin_xyz="${l2} 0 0"
    axis_xyz="0 0 1"
    limit_e="${le}" limit_l="${joint_lower}" limit_u="${joint_upper}" limit_v="${lv}" 
/>

<!-- Link 3 -->
<link_phantomx
    name="link_3"
    origin_rpy="${M_PI/2} 0 ${M_PI/2}" 
    origin_xyz="0 0 0"
    inertial_rpy="${M_PI/2} 0 ${M_PI/2}" 
    inertial_xyz="0.080988000 0.000023000 0.000748000"
    meshfile="package://phantomx_pincher/meshes/link_3.dae"
    mass="${link_3_mass}"
    ixx="${link_3_ixx}" ixy="${link_3_ixy}" ixz="${link_3_ixz}"
    iyy="${link_3_iyy}" iyz="${link_3_iyz}" izz="${link_3_izz}"	
/>  

<!-- Joint-Servomotor 4 -->
<!--le in % of 30Nm-->
<!--lv(revolute) in rad/s-->
<!--lv(prismatic) in m/s-->
<joint_phantomx
    name="joint_4" 
    type="revolute"
    parent="link_3" 
    child="link_4"
    origin_rpy="0 0 0" 
    origin_xyz="${l3} 0 0"
    axis_xyz="0 0 1"
    limit_e="${le}" limit_l="${joint_lower}" limit_u="${joint_upper}" limit_v="${lv}" 
/>

<!-- Link 4 -->
<link_phantomx
    name="link_4"
    origin_rpy="${M_PI/2} 0 ${M_PI/2}" 
    origin_xyz="0 0 0"
    inertial_rpy="${M_PI/2} 0 ${M_PI/2}" 
    inertial_xyz="0.047049000 -0.009712000 0.000023000"
    meshfile="package://phantomx_pincher/meshes/link_4.dae"
    mass="${link_4_mass}"
    ixx="${link_4_ixx}" ixy="${link_4_ixy}" ixz="${link_4_ixz}"
    iyy="${link_4_iyy}" iyz="${link_4_iyz}" izz="${link_4_izz}"	
/>  

<!-- Gripper Description -->

<!-- Joint-Servomotor-Gripper 1 -->
<joint_phantomx
    name="joint_finger_1" 
    type="prismatic"
    parent="link_4" 
    child="link_finger_1"
    origin_rpy="0 0 0" 
    origin_xyz="${l4} 0 -${offset_finger}"
    axis_xyz="0 0 1"
    limit_e="10" limit_l="0" limit_u="${joint_gripper_limit}" limit_v="0.001" 
/>

<!-- Joint-Servomotor-Gripper 2 -->
<joint_gripper_phantomx 
    name="joint_finger_2" 
    type="prismatic"
    parent="link_4"
    child="link_finger_2"
    origin_rpy="${M_PI} 0 0" 
    origin_xyz="${l4} 0 ${offset_finger}"
    axis_xyz="0 0 1"
    limit_e="10" limit_l="0" limit_u="${joint_gripper_limit}" limit_v="0.001" 
/>

